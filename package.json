{
  "name": "vsm",
  "version": "1.1.2",
  "main": "dist-electron/main/index.js",
  "description": "VSM is a detailed account manager for the game VALORANT.",
  "author": "synth",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder --publish always",
    "preview": "vite preview",
    "pretest": "vite build --mode=test",
    "test": "vitest run"
  },
  "build": {
    "appId": "com.synth.vsm",
    "productName": "VSM",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": "nsis",
      "artifactName": "${productName}_${version}.${ext}"
    },
    "publish": {
      "provider": "github",
      "owner": "synthofficial",
      "repo": "vsm",
      "releaseType": "release"
    }
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/serialize": "^1.3.3",
    "@emotion/utils": "^1.4.2",
    "@mantine/carousel": "^7.16.1",
    "@mantine/charts": "^7.16.1",
    "@mantine/core": "^7.16.1",
    "@mantine/emotion": "^7.16.1",
    "@mantine/hooks": "^7.16.1",
    "@mantine/notifications": "^7.16.1",
    "@tabler/icons-react": "^3.28.1",
    "@tqman/valorant-api-client": "^1.5.1",
    "@xhayper/discord-rpc": "^1.2.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "embla-carousel-react": "^7.1.0",
    "recharts": "2"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "electron": "^33.2.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.4.2",
    "vite": "^5.4.11",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^2.1.5"
  }
}
